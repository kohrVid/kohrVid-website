I"ü(function() {
  var adaptive, big, medium, small;

  small = function() {
    var cw, yp;
    $("#logo").attr("x", "0px");
    yp = (window.innerHeight - 35) + "px";
    $("#logo").attr("y", yp);
    cw = (window.innerWidth - 125) + "px";
    $(".container").css({
      left: "3px",
      width: cw
    });
    return $(".contact input, .contact textarea").css({
      width: "95%"
    });
  };

  medium = function() {
    var xp, yp;
    xp = (window.innerWidth - 370) + "px";
    $("#logo").attr("x", xp);
    yp = (window.innerHeight - 35) + "px";
    $("#logo").attr("y", yp);
    $(".container").css({
      left: "50px",
      width: "350px",
      height: "60%"
    });
    return $(".contact input, .contact textarea").css({
      width: "150%"
    });
  };

  big = function() {
    var xp, yp;
    xp = (window.innerWidth * 0.5) + "px";
    $("#logo").attr("x", xp);
    yp = (window.outerHeight - (window.innerHeight * 0.4)) + "px";
    $("#logo").attr("y", yp);
    $(".container").css({
      left: "50px",
      width: "400px",
      height: "60%"
    });
    return $(".contact input, .contact textarea").css({
      width: "180%"
    });
  };

  $(window).load(function() {
    $("#dropdown").hide();
    if (window.innerWidth < 500) {
      return small();
    } else if (window.innerWidth < 750) {
      return medium();
    } else {
      return big();
    }
  });

  adaptive = function() {
    if (window.innerWidth < 500) {
      return small();
    } else if (window.innerWidth < 750) {
      return medium();
    } else {
      return big();
    }
  };

  $(window).resize(function() {
    return adaptive();
  });

  $(document).ready(function() {
    return $("#menu").click(function() {
      return $("#dropdown").slideToggle();
    });
  });

  $(document).ready(function() {
    $(".close").click(function() {
      $(".alerts").hide();
      return $(".description").fadeOut();
    });
    return $(".close_image").click(function() {
      return $(".screenshot").fadeOut("slow");
    });
  });

}).call(this);
