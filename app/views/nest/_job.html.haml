%li.container-fluid
  .row
    .display-inline.col-lg-3.col-md-3.col-sm-3.col-xs-4
      %strong
        = link_to job.title, job.company_website
      %br
      #{job.start_date.strftime("%b %Y")} - #{job.end_date.strftime("%b %Y")}

    .display-inline.col-lg-9.col-md-9.col-sm-9.col-xs-8
      %br
      = job.description
