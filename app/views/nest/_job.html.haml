%li.container-fluid
  .row.p-t-1r
    .display-inline.col-lg-3.col-md-3.col-sm-3.col-xs-4
      %strong
        = link_to job.company_name, job.company_website
      %br
      #{job.start_date.strftime("%b %Y")} - #{job.format_end_date}

    .display-inline.col-lg-9.col-md-9.col-sm-9.col-xs-8
      = job.description
